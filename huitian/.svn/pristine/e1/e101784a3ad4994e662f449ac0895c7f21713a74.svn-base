package com.momathink.weixin.vo.oauth;

/**
 * 网页授权信息
 */
public class RecevieOauth2Token {
	private String access_token;// 网页授权接口调用凭证
	private int expires_in;// 凭证有效时长
	private String refresh_token;// 用于刷新凭证
	private String openId;// 用户标识
	private String scope;// 用户授权作用域

	private String errcode; // 错误码
	private String errmsg; // 错误提示

	public String getErrcode() {
		return errcode;
	}

	public void setErrcode(String errcode) {
		this.errcode = errcode;
	}

	public String getErrmsg() {
		return errmsg;
	}

	public void setErrmsg(String errmsg) {
		this.errmsg = errmsg;
	}

	public String getAccess_token() {
		return access_token;
	}

	public void setAccess_token(String access_token) {
		this.access_token = access_token;
	}

	public int getExpires_in() {
		return expires_in;
	}

	public void setExpires_in(int expires_in) {
		this.expires_in = expires_in;
	}

	public String getRefresh_token() {
		return refresh_token;
	}

	public void setRefresh_token(String refresh_token) {
		this.refresh_token = refresh_token;
	}

	public String getOpenId() {
		return openId;
	}

	public void setOpenId(String openId) {
		this.openId = openId;
	}

	public String getScope() {
		return scope;
	}

	public void setScope(String scope) {
		this.scope = scope;
	}
}